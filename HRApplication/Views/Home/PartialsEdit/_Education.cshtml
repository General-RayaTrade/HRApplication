

<form id="educationForm" >
    <fieldset>
        <h4>Educational background:</h4>
        <div>
                <p> High School:</p>

            <div class="form-row">
                <div class="form-group">

                    <label for="highgSchoolName" class="form-label">High School Name</label>
                    <input disabled type="text" required name="highgSchoolName" id="highgSchoolName" placeholder="" />
                </div>
                <div class="form-group">
                    <label for="highgSchoolDegree" class="form-label">Degree</label>


                    <select disabled name="highgSchoolDegree" id="highgSchoolDegree" required class="form-control">
                        <option value="Excellent">Excellent</option>
                        <option value="Very Good">Very Good</option>
                        <option value="Good">Good</option>
                        <option value="Fair">Fair</option>
                    </select>
                    
                </div>
            
            

           
            </div>
            <div class="form-row">
                <div class="form-group">

                    <label for="highgSchoolGraduationDate" class="form-label">Graduation Date</label>
                    <input disabled type="datetime" name="highgSchoolGraduationDate" required id="highgSchoolGraduationDate" placeholder="" />
                </div>
                <div class="form-group">

                    <label for="highgSchoolMajor" class="form-label">Major</label>
                    <input  disabled type="text" name="highgSchoolMajor" id="highgSchoolMajor" required placeholder="Ex. Computer Science" />
                </div>
            </div>
        </div> 
        <hr />
        <div>
            <p> Institute/ University:  </p>

            <div class="form-row" >
                <div class="form-group">
                    <label for="universityName" class="form-label">University Name</label>
                    <input disabled type="text" name="universityName" required id="universityName" placeholder="" />
                </div>
                <div class="form-group">
                    <label for="universityDegree" class="form-label">Degree</label>

                    <select disabled name="universityDegree" required id="universityDegree" class="form-control">
                        <option value="Excellent" >Excellent</option>
                        <option value="Very Good">Very Good</option>
                        <option value="Good">Good</option>
                        <option value="Fair">Fair</option>
                    </select>
                    
                </div>
                
            

           
            </div>
            <div class="form-row">
                <div class="form-group">

                    <label for="universityGraduationDate" class="form-label">Graduation Date</label>
                    <input disabled type="datetime" required name="universityGraduationDate" id="universityGraduationDate" placeholder="" />
                </div>
                <div class="form-group">

                    <label for="universityMajor" class="form-label">Major</label>
                    <input disabled type="text" required name="universityMajor" id="universityMajor" placeholder="Ex. Computer Science" />
                </div>

            </div>
        </div>
        <hr />
        <div class="form-row">
                <div class="form-group">
                <label for="additionalTraining">Additional Training/ Professional Qualifications:</label>
                <textarea disabled class="form-control" required name="additionalTraining" id="additionalTraining" rows="3"></textarea>
                </div>
           <div class="form-group">
                <label for="computerSkills" class="form-label">Computer Skills:  </label>

                <select disabled name="computerSkills" required id="computerSkills" class="form-control">
                    <option value="Excellent">Excellent</option>
                    <option value="Good">Good</option>
                    <option value="Fair">Fair</option>
                </select>

           </div>


        </div>
    </fieldset>
<button type="button" id="btnEducation" class="ml-3 btn btn-primary" style="background-color:#0011f6;margin-left:10px">Next</button>
</form>
<script>
    $(function () {

       

        $("#btnEducation").click(function () {
            window.location.replace(window.location.href.toString().split(window.location.hash)[0].concat("#collapseLang"))

                //window.location.replace(window.location.origin.concat("#collapseLang"));
           
        });
    });

</script>
